{
    "name": "tuzex/ecomai-bundle",
    "description": "Integration of ECOMAIL API",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Edo Barnas",
            "email": "iam@edobarnas.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Tuzex\\Ecomail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tuzex\\Ecomail\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "ecomailcz/ecomail": "^1.2",
        "symfony/framework-bundle": "^4.3|5.*|6.*|7.*"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpunit/phpunit": "^11.1",
        "symplify/easy-coding-standard": "^12.1"
    }
}
